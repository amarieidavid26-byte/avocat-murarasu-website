{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "css.lint.unknownAtRules": "ignore",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "emmet.includeLanguages": {
    "tailwindcss": "css"
  },
  "tailwindCSS.includeLanguages": {
    "tailwindcss": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    ["class[:]\\s*['\"`]([^'\"`]*)['\"`]", "([^'\"`]*)"]
  ]
}
